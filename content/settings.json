      "medium": "https://medium.com/@hirushachamod",
      "linkedin": "https://www.linkedin.com/in/hirusha-chamod/",
      "mail": "mailto:hirushachamod@gmail.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Portfolio" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Blogs", "url": "https://medium.com/@hirushachamod" },
        { "label": "Projects", "url": "/#features" },
        { "label": "Contact me", "url": "/#github" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Resume",
