{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Hirusha Chamod Portfolio",
    "titleTemplate": "%s Â· Portfolio Minimal",
    "description": "This is a work-in-progress project to easily get started with my own Portfolio.",
    "author": "Hirusha Chamod",
    "avatar": "../content/images/gatsby.png",
